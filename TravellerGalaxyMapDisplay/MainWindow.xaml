<Window x:Class="TravellerGalaxyMapDisplay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravellerGalaxyMapDisplay"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="800">
    <StackPanel>
        <Canvas Height="600" Width="800" Name="GalaxyCanvas"></Canvas>
        <Button Height="35" Click="DrawGalaxy">Start Simulation</Button>
    </StackPanel>
</Window>
